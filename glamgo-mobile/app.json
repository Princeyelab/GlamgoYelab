{
  "expo": {
    "name": "GlamGo",
    "slug": "glamgo-mobile",
    "scheme": "glamgo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E63946"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.glamgo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E63946"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.glamgo.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "40b9e40c-43c9-4b27-919f-d07088683ae5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/40b9e40c-43c9-4b27-919f-d07088683ae5"
    }
  }
}
